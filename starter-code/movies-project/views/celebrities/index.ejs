<h2><%= title %></h2>

<p class="">
  <a href="/celebrities/new">Create new</a>
</p>

<ul class="list">
  <% celebrities.forEach( (theCelebrity) => { %>
    <li>
      <a class="name" href="/celebrities/<%= theCelebrity._id %>"><%= theCelebrity.name %></a>
      <span class="occupation"><%= theCelebrity.occupation %></span>
      <a class="action edit" href="/celebrities/<%= theCelebrity._id %>/edit">Edit</a>
      <form class="actions" action="/celebrities/<%= theCelebrity._id %>/delete" method="POST">
        <button class="btn delete">Delete</button>
      </form>
    </li>
  <% }); %>
</ul>

